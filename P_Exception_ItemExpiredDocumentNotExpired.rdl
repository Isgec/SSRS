  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select distinct
tdmisg005200.t_cprj as Project,
tdmisg005200.t_cspa as Element,
tdmisg005200.t_docn as Document,
tdmisg005200.t_revn as Revision,
case tdmisg001200.t_stat 
when 1 then 'Submitted'
when 2 then 'Item Released'
when 3 then 'DRG Released'
when 4 then 'Expired'
end as DrawingStatus,
tdmisg001200.t_sdat as DrawingSubmissionDate
  from tdmisg005200 , tdmisg001200
where tdmisg001200.t_docn = tdmisg005200.t_docn 
and tdmisg001200.t_revn = tdmisg005200.t_revn
and tdmisg005200.t_stat &lt;&gt; tdmisg001200.t_stat 
and tdmisg005200.t_stat = 4
and tdmisg001200.t_stat &lt;&gt;4
and tdmisg001200.t_sdat &gt;= '01-Jan-2018'
union
select distinct
tdmisg005700.t_cprj as Project,
tdmisg005700.t_cspa as Element,
tdmisg005700.t_docn as Document,
tdmisg005700.t_revn as Revision,
case tdmisg001700.t_stat 
when 1 then 'Submitted'
when 2 then 'Item Released'
when 3 then 'DRG Released'
when 4 then 'Expired'
end as DrawingStatus,
tdmisg001700.t_sdat as DrawingSubmissionDate
  from tdmisg005700 , tdmisg001700
where tdmisg001700.t_docn = tdmisg005700.t_docn 
and tdmisg001700.t_revn = tdmisg005700.t_revn
and tdmisg005700.t_stat &lt;&gt; tdmisg001700.t_stat 
and tdmisg005700.t_stat = 4
and tdmisg001700.t_stat &lt;&gt;4
and tdmisg001700.t_sdat &gt;= '01-Jan-2018'
union
select  distinct
tdmisg005651.t_cprj as Project,
tdmisg005651.t_cspa as Element,
tdmisg005651.t_docn as Document,
tdmisg005651.t_revn as Revision,
case tdmisg001651.t_stat 
when 1 then 'Submitted'
when 2 then 'Item Released'
when 3 then 'DRG Released'
when 4 then 'Expired'
end as DrawingStatus,
tdmisg001651.t_sdat as DrawingSubmissionDate
  from tdmisg005651 , tdmisg001651
where tdmisg001651.t_docn = tdmisg005651.t_docn 
and tdmisg001651.t_revn = tdmisg005651.t_revn
and tdmisg005651.t_stat &lt;&gt; tdmisg001651.t_stat 
and tdmisg005651.t_stat = 4
and tdmisg001651.t_stat &lt;&gt;4
and tdmisg001651.t_sdat &gt;= '01-Jan-2018'</CommandText>
